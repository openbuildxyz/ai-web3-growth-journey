{
  "apiMappings": [
    {
      "mappingId": "mapping-users-crud",
      "backendEndpoint": "/api/v1/users",
      "mcpServiceId": "mcp-service-users",
      "translationRules": [
        {
          "ruleId": "rule-users-get",
          "pattern": "get user with id {id}",
          "targetEndpoint": "/api/v1/users/{id}",
          "parameterMappings": {
            "id": "id"
          },
          "transformFunction": "defaultTransform"
        },
        {
          "ruleId": "rule-users-list",
          "pattern": "get all users",
          "targetEndpoint": "/api/v1/users",
          "parameterMappings": {},
          "transformFunction": "defaultTransform"
        },
        {
          "ruleId": "rule-users-create",
          "pattern": "create user with name {name} and email {email}",
          "targetEndpoint": "/api/v1/users",
          "parameterMappings": {
            "name": "name",
            "email": "email"
          },
          "transformFunction": "defaultTransform"
        }
      ],
      "naturalLanguagePatterns": [
        "Get user information",
        "Find user by ID",
        "List all users",
        "Show users",
        "Create new user",
        "Add user to system"
      ],
      "enabled": true
    },
    {
      "mappingId": "mapping-products-crud",
